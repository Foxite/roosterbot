{
  "metadata": [
    {
      "src": [
        {
		  "src": ".",
          "files": [
            "**/RoosterBot.csproj",
            "**/RoosterBot.DiscordNet.csproj",
            "**/RoosterBot.Telegram.csproj",
            "**/RoosterBot.Console.csproj",
            "**/RoosterBot.Console.App.csproj",
            "**/RoosterBot.DateTimeUtils.csproj",
            "**/RoosterBot.AWS.csproj",
            "**/RoosterBot.Meta.csproj",
            "**/RoosterBot.Schedule.csproj",
            "**/RoosterBot.Statistics.csproj"
          ],
		  "exclude": [
            "**/bin/**",
            "**/obj/**",
            "_site/**"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}